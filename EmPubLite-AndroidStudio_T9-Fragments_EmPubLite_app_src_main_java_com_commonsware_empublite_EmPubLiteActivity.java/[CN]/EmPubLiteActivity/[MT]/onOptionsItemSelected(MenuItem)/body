{
switch (item.getItemId()) {
case android.R.id.home:
    return (true);
case R.id.about:
  Intent i=new Intent(this,SimpleContentActivity.class);
startActivity(i);
return (true);
case R.id.help:
i=new Intent(this,SimpleContentActivity.class);
startActivity(i);
return (true);
}
return (super.onOptionsItemSelected(item));
}

{
  assertNotNull(ImageRequest.class.getConstructor(String.class,Response.Listener.class,int.class,int.class,Bitmap.Config.class,Response.ErrorListener.class));
  assertNotNull(ImageRequest.class.getConstructor(String.class,Response.Listener.class,int.class,int.class,ImageView.ScaleType.class,Bitmap.Config.class,Response.ErrorListener.class));
  assertEquals(ImageRequest.DEFAULT_IMAGE_TIMEOUT_MS,1000);
  assertEquals(ImageRequest.DEFAULT_IMAGE_MAX_RETRIES,2);
  assertEquals(ImageRequest.DEFAULT_IMAGE_BACKOFF_MULT,2f);
}

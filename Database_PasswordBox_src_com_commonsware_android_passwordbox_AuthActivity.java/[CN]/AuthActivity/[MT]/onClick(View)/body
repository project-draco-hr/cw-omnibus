{
  v.setEnabled(false);
  if (dbState == State.UNENCRYPTED) {
    try {
      DatabaseHelper.encrypt(this,passphrase.getText().toString());
    }
 catch (    IOException e) {
      Toast.makeText(this,getString(R.string.problem_encrypting_database) + e.getLocalizedMessage(),Toast.LENGTH_LONG).show();
      finish();
      return;
    }
  }
  DatabaseHelper.initDatabase(this,passphrase.getText().toString());
  startActivity(new Intent(this,MainActivity.class));
  finish();
}

{
  form=getActivity().getLayoutInflater().inflate(R.layout.dialog,null);
  AlertDialog.Builder builder=new AlertDialog.Builder(getActivity());
  return (builder.setTitle(R.string.dlg_title).setView(form).setPositiveButton(android.R.string.ok,this).setNegativeButton(android.R.string.cancel,null).create());
}

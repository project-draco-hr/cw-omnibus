{
  if (rowId > 0) {
    new AlertDialog.Builder(this).setTitle(R.string.delete_title).setPositiveButton(R.string.ok,new DialogInterface.OnClickListener(){
      public void onClick(      DialogInterface dialog,      int whichButton){
        processDelete(rowId);
      }
    }
).setNegativeButton(R.string.cancel,new DialogInterface.OnClickListener(){
      public void onClick(      DialogInterface dialog,      int whichButton){
      }
    }
).show();
  }
}

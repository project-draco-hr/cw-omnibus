{
  NotificationCompat.Builder b=new NotificationCompat.Builder(this);
  b.setAutoCancel(true).setDefaults(Notification.DEFAULT_ALL).setContentTitle(getString(R.string.entry)).setContentIntent(buildPendingIntent(Settings.ACTION_SECURITY_SETTINGS)).setSmallIcon(android.R.drawable.stat_sys_download_done).setTicker(getString(R.string.download_complete)).setGroup(GROUP_SAMPLE);
  mgr.notify(NOTIFY_ID2,b.build());
}
